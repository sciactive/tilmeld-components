{
  "name": "tilmeld-components",
  "version": "1.0.0",
  "description": "Nymph user and group management with access controls.",
  "main": "dist/TilmeldComponents.js",
  "module": "lib/index.js",
  "scripts": {
    "compile": "node make.js && cp src/index.js lib/",
    "pack": "webpack",
    "build": "npm run compile && npm run pack",
    "lint": "prettier \"./{*.js,src/**/*.{html,svelte,js,css,scss}}\"",
    "format": "prettier --write \"./{*.js,src/**/*.{html,svelte,js,css,scss}}\"",
    "prepare": "npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/tilmeld-components.git"
  },
  "keywords": [
    "users",
    "user management",
    "user manager",
    "groups",
    "group management",
    "group manager",
    "acl",
    "access control",
    "nymph"
  ],
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/tilmeld-components/issues"
  },
  "homepage": "https://github.com/sciactive/tilmeld-components#readme",
  "dependencies": {
    "nymph-client": "^4.0.0",
    "pform": "^3.3",
    "tilmeld-client": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@tivac/eslint-plugin-svelte": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "prettier": "^1.17.1",
    "prettier-plugin-svelte": "^0.6.0",
    "svelte": "^3.4.4",
    "svelte-loader": "^2.13.4",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  }
}
